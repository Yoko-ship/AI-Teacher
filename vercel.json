{
    "version": 2,
    "builds": [
      {
        "src": "backend/app.js",
        "use": "@vercel/node"
      },
      {
        "src": "package.json",
        "use": "@vercel/static-build"
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "backend/app.js"
      },
      {
        "src": "/(.*)",
        "dest": "backend/app.js"
      }
    ]
  }